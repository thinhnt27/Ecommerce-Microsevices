      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      DatabaseSettings__BrandsCollection: Brands
      DatabaseSettings__CollectionName: Products
      DatabaseSettings__ConnectionString: mongodb://catalogdb:27017
      DatabaseSettings__DatabaseName: CatalogDb
      DatabaseSettings__TypesCollection: Types
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net8.0/Catalog.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        published: "8000"
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: Z:\Code\.NET\Ecommerce\Services\Catalog\Catalog.API
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: Z:\Code\.NET\Ecommerce
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\PCIT\vsdbg\vs2017u5
